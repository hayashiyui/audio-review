{
  "name": "",
  "type": "module",
  "version": "1.6.2",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "prettier": "prettier --write **/*.{ts,tsx,css,astro} --ignore-path .gitignore",
    "postinstall": "patch-package",
    "postbuild": "pagefind --site dist --exclude-selectors \"pre, code, .expressive-code, .ec-line, script, style, .pagefind-exclude\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/markdown-remark": "^6.3.6",
    "@astrojs/mdx": "^4.3.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.5.1",
    "@expressive-code/plugin-collapsible-sections": "^0.40.2",
    "@expressive-code/plugin-line-numbers": "^0.40.2",
    "@iconify-json/lucide": "^1.2.66",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "astro": "^5.13.5",
    "astro-expressive-code": "^0.40.2",
    "astro-icon": "^1.1.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.469.0",
    "patch-package": "^8.0.0",
    "radix-ui": "^1.4.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "recharts": "^3.1.2",
    "rehype-document": "^7.0.3",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.1",
    "rehype-pretty-code": "^0.14.1",
    "remark-emoji": "^5.0.2",
    "remark-math": "^6.0.0",
    "sharp": "^0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "pagefind": "^1.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-tailwindcss": "^0.6.14"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-astro",
      "prettier-plugin-tailwindcss",
      "prettier-plugin-astro-organize-imports"
    ],
    "overrides": [
      {
        "files": "*.astro",
        "options": {
          "parser": "astro"
        }
      }
    ]
  }
}
